@model IEnumerable<Airlines.Data.Entities.RaceTeam>

@{
    ViewBag.Title = "View";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>View</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PilotID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NavigatorID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RadioManID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsAccepted)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PilotID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NavigatorID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RadioManID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsAccepted)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
